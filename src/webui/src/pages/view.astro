---
import Layout from '@/components/base.astro';
import Navbar from '@/components/navbar.astro';
import ViewPage from '@/components/react/view';
import { SITE_TITLE, SITE_DESCRIPTION } from '@/consts';
---

<Layout title={SITE_TITLE + " - Viewing process {{process_id | safe}}"} description={SITE_DESCRIPTION}>
   <Navbar />
   <main class="min-h-screen bg-zinc-950 pt-20">
      <div class="py-8">
         <ViewPage id={"{{process_id | safe}}"} base={"{{base_path | safe}}"} client:only="@astrojs/react" />
      </div>
   </main>
</Layout>
