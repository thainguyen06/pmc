---
import Layout from '@/components/base.astro';
import Navbar from '@/components/navbar.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '@/consts';
---

<Layout title={SITE_TITLE + " - API Docs"} description={SITE_DESCRIPTION}>
	<Navbar active={"docs"} />
	<main class="pt-12 sm:pt-10 h-screen">
		<div class="embed-container">
			<iframe src={"{{base_path | safe}}/docs/embed"} frameborder="0" allowfullscreen></iframe>
		</div>
	</main>
</Layout>
