---
import Layout from '@/components/base.astro';
import Navbar from '@/components/navbar.astro';
import IndexPage from '@/components/react/index';
import { SITE_TITLE, SITE_DESCRIPTION } from '@/consts';
---

<Layout title={SITE_TITLE + " - Process list"} description={SITE_DESCRIPTION}>
	<Navbar active={"list"} />
	<main class="min-h-screen bg-zinc-950 pt-20">
		<div class="py-8">
			<IndexPage base={"{{base_path | safe}}"} client:only="@astrojs/react" />
		</div>
	</main>
</Layout>
