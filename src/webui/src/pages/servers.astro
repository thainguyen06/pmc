---
import Layout from '@/components/base.astro';
import Navbar from '@/components/navbar.astro';
import ServersPage from '@/components/react/servers';
import { SITE_TITLE, SITE_DESCRIPTION } from '@/consts';
---

<Layout title={SITE_TITLE + " - Agents"} description={SITE_DESCRIPTION}>
	<Navbar active={"servers"} />
	<main class="min-h-screen bg-zinc-950 pt-20">
		<div class="py-8">
			<ServersPage base={"{{base_path | safe}}"} client:only="@astrojs/react" />
		</div>
	</main>
</Layout>
